{
    "name": "leebiz473/bootstrappenvx",
    "type": "project",
    "description": "Bootstrap App Environment using Laravel 10 scaffold",
    "keywords": [
        "bootstrap",
        "app",
        "environment"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.3",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "brainmaestro/composer-git-hooks": "3.0.0-alpha.1",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.14",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "php-parallel-lint/php-console-highlighter": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^10.1",
        "psalm/plugin-laravel": "^2.7",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Foundation\\": "src/Foundation/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "composer cghooks",
            "composer ide-helper"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "composer cghooks",
            "composer ide-helper"
        ],
        "phpcs": "chmod +x .tools/phpcs.sh && .tools/phpcs.sh",
        "phpcbf": "chmod +x .tools/phpcbf.sh && .tools/phpcbf.sh",
        "lint": "chmod +x .tools/parallel_lint.sh && .tools/parallel_lint.sh",
        "fixer": "php-cs-fixer fix app --rules=@PSR12",
        "phpcpd": "chmod +x phpcpd.phar && .tools/phpcpd.sh",
        "phpmd": "chmod +x .tools/phpmd.sh && .tools/phpmd.sh",
        "phpstan": "phpstan analyse -c phpstan.neon --memory-limit 1G --xdebug --ansi",
        "psalm": "psalm",
        "cghooks": "[ -f vendor/bin/cghooks ] && vendor/bin/cghooks update || echo 'cghooks not found'",
        "ide-helper": [
            "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:generate || echo 'IDE helper not found'",
            "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:meta || echo 'IDE helper not found'"
        ],
        "ide-doc-models": "[ -d vendor/barryvdh/laravel-ide-helper ] && php artisan ide-helper:models || echo 'IDE helper not found'"
    },
    "extra": {
        "hooks": {
            "config": {
                "stop-on-failure": [
                    "pre-commit",
                    "pre-push"
                ]
            },
            "pre-commit": "chmod +x .hooks/pre_commit.sh && .hooks/pre_commit.sh",
            "pre-push": "chmod +x .hooks/pre_push.sh && .hooks/pre_push.sh"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
